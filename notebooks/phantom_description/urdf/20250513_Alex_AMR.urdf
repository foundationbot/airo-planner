			<mass value="80.0"/>
			<origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
			<inertia ixx="0.5" ixy="0.0" ixz="0.5" iyy="0.0" iyz="0.0" izz="0.5"/>
