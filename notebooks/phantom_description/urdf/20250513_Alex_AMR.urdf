		<inertial>
			<mass value="0"/>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0" />
		</inertial>
<!--		<visual>-->
<!--			<geometry>-->
<!--				<mesh filename="package://agile_x_ranger_amr_description/meshes/Agile_X_Ranger_Base.obj"/>-->
<!--			</geometry>-->
<!--			<origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.0"/>-->
<!--		</visual>-->
	</link>
	
